{
  "name": "vue3-moveable-dashboard",
  "version": "1.0.0",
  "description": "A Vue 3 component library for creating draggable, resizable, and rotatable dashboard cards with Vuetify using native browser events",
  "author": "",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "dashboard",
    "drag-and-drop",
    "draggable",
    "resizable",
    "floating",
    "vuetify",
    "component"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/style.css": "./dist/vue3-moveable-dashboard.css",
    "./dist/vue3-moveable-dashboard.css": "./dist/vue3-moveable-dashboard.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.14",
    "vue-tsc": "^2.2.10",

    "vuetify": "^3.8.0-beta.0"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue-router": "^4.6.3"
  }
}
